class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        
        common = min(strs, key=len)
        
        for string in strs:
            while not common == "":
                if common in string[0:len(common)]:
                    break
                else: 
                    common = common[:-1]
        
        return common
        
